{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Query Script",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/src/qurery.js",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "env": {
        "NODE_ENV": "development"
      },
      "runtimeArgs": ["--enable-source-maps"]
    },
    {
      "name": "Debug Seed Script",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/src/index.js",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "env": {
        "NODE_ENV": "development"
      },
      "runtimeArgs": ["--enable-source-maps"]
    },
    {
      "name": "Debug Current File",
      "type": "node",
      "request": "launch",
      "program": "${file}",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "env": {
        "NODE_ENV": "development"
      },
      "runtimeArgs": ["--enable-source-maps"]
    }
  ]
}
